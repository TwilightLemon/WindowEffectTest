<Application x:Class="WindowEffectTest.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WindowEffectTest"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="IconData.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="ForeColor" Color="White"/>
            <SolidColorBrush x:Key="MaskColor" Color="#0CFFFFFF"/>
            <SolidColorBrush x:Key="FocusMaskColor" Color="#19FFFFFF"/>

        </ResourceDictionary>
    </Application.Resources>
</Application>
